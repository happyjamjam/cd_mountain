<h2> 住所編集</h2>
	<%= form_for(@address) do |f| %>
		<%= f.label :name, "氏名" %>
		<%= f.text_field :name %>

		<%= f.label :postal_code, "郵便番号" %>
		<%= f.text_field :postal_code %>

		<%= f.label :address, "住所" %>
		<%= f.text_field :address %>

		<%= f.label :tel, "電話番号" %>
		<%= f.text_field :tel %>

		<%= f.submit "編集完了" %>

	<% end %>